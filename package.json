{
  "name": "gb-se-ts",
  "version": "1.0.0",
  "description": "Test project 1",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "postinstall": "npx serenity-bdd update",
    "clean": "rimraf target",
    "test": "failsafe clean test:execute test:report",
    "test:execute-prd": "cucumber-js ./dist/src/features",
